{
    "name": "andkom/ethereum-address",
    "description": "A simple Ethereum address encoder/validator written in PHP.",
    "type": "library",
    "keywords": [
        "ethereum",
        "address",
        "encoder",
        "generator",
        "validator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrei Komarov"
        }
    ],
    "require": {
        "php": "^7.0 | ^8.0",
        "kornrunner/keccak": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "AndKom\\Ethereum\\Address\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AndKom\\Ethereum\\Address\\Tests\\": "tests/"
        }
    }
}
